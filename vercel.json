{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ]
}